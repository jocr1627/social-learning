{
  "name": "social-learning",
  "version": "0.0.0",
  "description": "A social media platform dedicated to education.",
  "main": "server.js",
  "scripts": {
    "build": "npm run clean && npm run relay && webpack",
    "clean": "rm -rf src/__generated__ && rm -rf dist",
    "dev": "npm run build && webpack-dev-server --open",
    "lint": "eslint . --fix q--max-warnings=0 && echo 'No errors' || echo 'Finished with errors'",
    "relay": "relay-compiler --src ./src --schema schema.graphql",
    "start": "node ./server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "John Cronk <john.a.cronk@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-relay": "^1.4.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.7.2",
    "eslint-plugin-react": "^7.4.0",
    "relay-compiler": "^1.4.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "express": "^4.16.0",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.6",
    "linkify-it": "^2.0.3",
    "material-ui": "^0.19.2",
    "pg": "^7.3.0",
    "pg-native": "^2.2.0",
    "proptypes": "^1.1.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-relay": "^1.4.1",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.6.1",
    "tlds": "^1.197.0",
    "url-regex": "^4.1.1",
    "whatwg-fetch": "^2.0.3"
  }
